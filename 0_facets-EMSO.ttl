#########
# EMSO Concept Scheme
#########

<category:EMSO> a skos:ConceptScheme ;
       skos:prefLabel "EMSO" ;
       dct:title "EMSO" ;
       dct:description "EMSO Domain" ;
       foaf:logo "https://emso.eu/wp-content/uploads/2018/04/emso-logo2-400x270.jpg" ;
       #foaf:homepage "" ;
       schema:color "#9261b7";
       schema:orderItemNumber "12";
       skos:hasTopConcept <category:facets/dataset-theme> ;
 .

#########
# Concepts
#########

<category:EMSO_conc> a skos:Concept ;
        skos:inScheme    <category:EMSO> ;
        skos:definition  "EMSO categories" ;
        skos:prefLabel   "EMSO categories"
 .
